    "mag1 = 20*log10(abs(H))\n",
    "clf()\n",
    "figure(1)\n",
    "plot(f, mag1, lineWidth=2)\n",
    "title('Janela de Hanning')\n",
    "axis([0, ws/2, -100, 10])\n",
    "xlabel('$\\omega$',fontsize=14)\n",
    "ylabel('$|H(\\omega)|$',fontsize=14)\n",
    "grid(True)\n",
    "show()"
   ]
